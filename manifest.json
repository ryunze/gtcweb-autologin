{
    "manifest_version": 3,
    "name": "Cookie Editor with HttpOnly Support",
    "version": "1.0",
    "permissions": [
        "cookies",
        "activeTab"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "host_permissions": [
        "https://web.getcontact.com/",
        "https://*.getcontact.com/*"
    ]
}